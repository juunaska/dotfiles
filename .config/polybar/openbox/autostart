cpupower-gui pr superfast && cpupower-gui -p &
xrandr -o right &
polybar -r &
nitrogen --restore &
setxkbmap fi &
xfce4-power-manager &
xinput --set-prop 12 'libinput Accel Speed' 0.15 &
xinput set-prop "SynPS/2 Synaptics TouchPad" "libinput Tapping Enabled" 1 &
xinput set-prop "12" "libinput Tapping Enabled" 1 &

